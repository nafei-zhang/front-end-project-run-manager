# Electron 离线安装配置
# 将此文件重命名为 .npmrc 以启用离线模式

# 禁用网络请求
offline=true

# Electron 配置
target_platform=win32
target_arch=x64
cache=./electron-cache
electron_mirror=file://./electron-cache/

# 禁用进度条和更新检查
progress=false
update-notifier=false

# 使用本地缓存
prefer-offline=true
cache-min=86400

# 禁用审计和资金信息
audit=false
fund=false